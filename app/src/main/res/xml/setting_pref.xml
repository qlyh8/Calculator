<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="@string/pref_theme_value_black"
        android:key="@string/pref_theme_key"
        android:entries="@array/pref_theme_option_labels"
        android:entryValues="@array/pref_theme_option_values"
        android:title="@string/pref_theme_label" />
    <CheckBoxPreference
        android:defaultValue="@bool/pref_vibration_default"
        android:key="@string/pref_vibration_key"
        android:summaryOff="@string/pref_vibration_false"
        android:summaryOn="@string/pref_vibration_true"
        android:title="@string/pref_vibration_label" />
</PreferenceScreen>

